
        {{define "title"}}Home{{end}}

        {{define "main"}}
    <h2>Latest Snippets</h2>
    {{if .Snippets}}
    <table>
        <tr>
            <th>Title</th>
            <th>Created Date</th>
            <th>ID</th>
        </tr>
        {{range .Snippets}}
        <tr>
            <td>{{.Title}}</td>
            <td>{{.Created}}</td>
            <td>{{.ID}}</td>
        </tr>
        {{end}}
    </table>
    {{else}}
    <p>There's nothing to see here yet!</p>
    {{end}}
    {{end}}
